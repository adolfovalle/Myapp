<h1>Listing palabras_tweets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @palabras_tweets.each do |palabras_tweet| %>
      <tr>
        <td><%= link_to 'Show', palabras_tweet %></td>
        <td><%= link_to 'Edit', edit_palabras_tweet_path(palabras_tweet) %></td>
        <td><%= link_to 'Destroy', palabras_tweet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Palabras tweets', new_palabras_tweet_path %>
